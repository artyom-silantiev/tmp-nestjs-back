{
  "name": "tmp-nestjs-back",
  "private": true,
  "version": "0.6.3",
  "description": "",
  "scripts": {
    "devs": "prisma migrate dev && npm run cli:seeder",
    "migrate_dev": "prisma migrate dev",
    "migrate_prod": "npx prisma generate && prisma migrate deploy",
    "prebuild": "rimraf dist",
    "build:all": "npm run build:server",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "CLI": "echo 'cli block'",
    "cli:seeder": "CLI_PATH=src/app_cli/cli.ts nestjs-command seeder",
    "cli:cluster:list": "CLI_PATH=src/app_cli/cli.ts nestjs-command cluster:list",
    "cli:cluster:update_traefik_config": "CLI_PATH=src/app_cli/cli.ts nestjs-command cluster:update_traefik_config",
    "WEB": "echo 'WEB block'",
    "build:web": "nest build web",
    "start:web": "npx nest start web",
    "start:web:dev": "npx nest start web --debug --watch",
    "start:web:prod": "NO_COLOR=1 node dist/src/app_web/main"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/websockets": "^9.2.1",
    "@prisma/client": "^4.7.1",
    "aws-sdk": "^2.1270.0",
    "axios": "^1.2.1",
    "base-x": "^4.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.0",
    "hasha": "^5.2.2",
    "ioredis": "^5.2.4",
    "json-to-pretty-yaml": "^1.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multihashes": "^4.0.3",
    "nestjs-command": "^3.1.2",
    "nestjs-i18n": "^9.2.3",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.6.0",
    "sharp": "^0.31.2",
    "socket.io-client": "^4.5.4",
    "socket.io-redis": "^6.1.1",
    "swagger-ui-express": "^4.6.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.11",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/my-library(|/.*)$": "<rootDir>/libs/my-library/src/$1",
      "^@app/db(|/.*)$": "<rootDir>/libs/db/src/$1",
      "^@app/share(|/.*)$": "<rootDir>/libs/share/src/$1",
      "^@app/env(|/.*)$": "<rootDir>/libs/env/src/$1",
      "^@app/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@app/i18n(|/.*)$": "<rootDir>/libs/i18n/src/$1"
    }
  }
}
