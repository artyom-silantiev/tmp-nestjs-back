{
  "name": "tmp-nestjs-back",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "devs": "prisma migrate dev && npm run cli:seeder",
    "migrate_dev": "prisma migrate dev",
    "migrate_prod": "npx prisma generate && prisma migrate deploy",
    "prebuild": "rimraf dist",
    "build:all": "npm run build:server",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "CLI": "echo 'cli block'",
    "cli:seeder": "CLI_PATH=_apps/cli/src/cli.ts nestjs-command seeder",
    "cli:cluster:list": "CLI_PATH=_apps/cli/src/cli.ts nestjs-command cluster:list",
    "cli:cluster:update_traefik_config": "CLI_PATH=_apps/cli/src/cli.ts nestjs-command cluster:update_traefik_config",
    "WEB": "echo 'WEB block'",
    "build:web": "nest build",
    "start:web": "npx nest start",
    "start:web:dev": "npx nest start --debug --watch",
    "start:web:prod": "node dist/_apps/web/main"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.8",
    "@nestjs/core": "^9.0.8",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/platform-socket.io": "^9.0.8",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/websockets": "^9.0.8",
    "@prisma/client": "^4.1.1",
    "aws-sdk": "^2.1187.0",
    "axios": "^0.27.2",
    "base-x": "^4.0.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "file-type": "^17.1.6",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.1.0",
    "hasha": "^5.2.2",
    "ioredis": "^5.2.2",
    "json-to-pretty-yaml": "^1.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multihashes": "^4.0.3",
    "nestjs-command": "^3.1.2",
    "nestjs-i18n": "^9.1.5",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "sharp": "^0.30.7",
    "socket.io-client": "^4.5.1",
    "socket.io-redis": "^6.1.1",
    "swagger-ui-express": "^4.5.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.7",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sharp": "^0.30.4",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/my-library(|/.*)$": "<rootDir>/libs/my-library/src/$1",
      "^@app/db(|/.*)$": "<rootDir>/libs/db/src/$1",
      "^@app/share(|/.*)$": "<rootDir>/libs/share/src/$1",
      "^@app/env(|/.*)$": "<rootDir>/libs/env/src/$1",
      "^@app/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@app/i18n(|/.*)$": "<rootDir>/libs/i18n/src/$1"
    }
  }
}