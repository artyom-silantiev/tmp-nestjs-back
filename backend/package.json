{
  "name": "tmp-nestjs-back",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "devs": "prisma migrate dev && npm run cli:seeder",
    "migrate_dev": "prisma migrate dev",
    "migrate_prod": "npx prisma generate && prisma migrate deploy",
    "prebuild": "rimraf dist",
    "build:all": "npm run build:server",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "CLI": "echo 'cli block'",
    "cli:seeder": "CLI_PATH=_apps/cli/src/cli.ts nestjs-command seeder",
    "cli:cluster:list": "CLI_PATH=apps/cli/src/cli.ts nestjs-command cluster:list",
    "cli:cluster:update_traefik_config": "CLI_PATH=apps/cli/src/cli.ts nestjs-command cluster:update_traefik_config",
    "SERVER": "echo 'server block'",
    "build:web": "nest build",
    "start:web": "npx nest start",
    "start:web:dev": "npx nest start --debug --watch",
    "start:web:prod": "node dist/_apps/web/main"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.6",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/platform-socket.io": "^8.4.6",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/websockets": "^8.4.6",
    "@prisma/client": "^3.14.0",
    "aws-sdk": "^2.1077.0",
    "axios": "^0.27.2",
    "base-x": "^3.0.9",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "file-type": "^16.5.3",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.1.0",
    "handy-redis": "^2.3.1",
    "hasha": "^5.2.2",
    "json-to-pretty-yaml": "^1.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "multihashes": "^4.0.3",
    "nestjs-command": "^3.1.1",
    "nestjs-i18n": "^8.2.2",
    "nodemailer": "^6.7.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sharp": "^0.30.5",
    "socket.io-client": "^4.5.1",
    "socket.io-redis": "^6.1.1",
    "swagger-ui-express": "^4.4.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.2",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sharp": "^0.30.2",
    "@types/supertest": "^2.0.11",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/my-library(|/.*)$": "<rootDir>/libs/my-library/src/$1",
      "^@app/db(|/.*)$": "<rootDir>/libs/db/src/$1",
      "^@app/share(|/.*)$": "<rootDir>/libs/share/src/$1",
      "^@app/env(|/.*)$": "<rootDir>/libs/env/src/$1",
      "^@app/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@app/i18n(|/.*)$": "<rootDir>/libs/i18n/src/$1"
    }
  }
}